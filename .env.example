# ===========================================
# PostgreSQL
# ===========================================
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password_here
POSTGRES_HOST=db
POSTGRES_PORT=5432

# ===========================================
# n8n ETL
# ===========================================
N8N_BASIC_AUTH_USER=your_username
N8N_BASIC_AUTH_PASSWORD=your_password

# ===========================================
# ngrok (tunnel for webhook access)
# ===========================================
NGROK_AUTHTOKEN=your_ngrok_authtoken
NGROK_DOMAIN=your-subdomain.ngrok-free.dev

# ===========================================
# n8n Webhook (for Discord bot integration)
# ===========================================
N8N_WEBHOOK_URL=https://your-subdomain.ngrok-free.dev/webhook/your-webhook-id

# ===========================================
# Discord Bot
# ===========================================
DISCORD_TOKEN=your_discord_bot_token
DISCORD_CLIENT_ID=your_application_client_id
DISCORD_GUILD_ID=your_server_guild_id
